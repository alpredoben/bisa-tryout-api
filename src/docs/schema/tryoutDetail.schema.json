{
  "SchemaCreateForTryoutDetail": {
    "type": "object",
    "properties": {
      "package_id": {
        "type": "string",
        "summary": "Tryout Package ID",
        "default": "UUID"
      },
      "type_id": {
        "type": "string",
        "summary": "Tryout Type ID",
        "default": "UUID"
      },
      "total_questions": {
        "type": "number",
        "summary": "Total Question",
        "default": 110
      },
      "total_duration": {
        "type": "number",
        "summary": "Total Duration",
        "default": 110
      },
      "satuan_duration": {
        "type": "string",
        "summary": "Satuan (menit, detik, jam)",
        "default": "detik"
      },
      "passing_grade": {
        "type": "number",
        "default": 100
      },
      "order_number": {
        "type": "number",
        "default": 1
